    "#SEQUENCE OPERATION: MODULATION\n",
    "\n",
    "xn = [51, 52, 53, 54, 55]\n",
    "yn = [1, 2, 3, 4, 5]\n",
    "def modulation(x, y):\n",
    "    wn = []  \n",
    "    for i in range(len(x)):\n",
    "        wn.append(x[i] * y[i])\n",
    "    return wn\n",
    "wn = modulation(xn, yn)\n",
    "print(\"Modulation Product:\", wn)"
